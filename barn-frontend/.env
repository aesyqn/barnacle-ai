# Barnacle AI Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8000/api

# App Configuration
VITE_APP_NAME=Barnacle AI
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEV_MODE=true
VITE_ENABLE_DEVTOOLS=true

# TanStack Query Configuration
VITE_QUERY_STALE_TIME=300000
VITE_QUERY_CACHE_TIME=600000

# Authentication Configuration
VITE_TOKEN_STORAGE_KEY=authToken
VITE_REFRESH_TOKEN_KEY=refreshToken

# Maritime Data Refresh Intervals (in milliseconds)
VITE_AIS_REFRESH_INTERVAL=30000
VITE_WEATHER_REFRESH_INTERVAL=180000
VITE_FLEET_REFRESH_INTERVAL=120000
VITE_ESG_REFRESH_INTERVAL=1800000

# Notification Configuration
VITE_NOTIFICATION_DURATION=4000
VITE_MAX_NOTIFICATIONS=3